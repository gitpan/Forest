<!DOCTYPE Component SYSTEM "../Oak/Filer/Component.dtd">
  <main>
    <prop name="name" value="actionPage"/>
    <prop name="title" value="Editing your page"/>
    <prop name="linkrel" value="link rel='stylesheet' type='text/css' href='/style.css'"/>
    <owned name="frameset">
      <prop name="__CLASSNAME__" value="Oak::Web::HTML::Frameset"/>
      <prop name="parent" value="actionPage"/>
      <prop name="top" value="0"/>
      <prop name="left" value="0"/>
      <prop name="rows" value="100,*"/>
    </owned>
    <owned name="newCompFrame">
      <prop name="__CLASSNAME__" value="Oak::Web::Additional::ActionFrame"/>
      <prop name="parent" value="frameset"/>
      <prop name="top" value="0"/>
      <prop name="left" value="0"/>
      <prop name="ev_onLoad" value="$::TL::actionPage->on_newComp_click"/>
    </owned>
    <owned name="subFrameset">
      <prop name="__CLASSNAME__" value="Oak::Web::HTML::Frameset"/>
      <prop name="parent" value="frameset"/>
      <prop name="top" value="1"/>
      <prop name="left" value="0"/>
      <prop name="cols" value="300,*,300"/>
      <prop name="id" value="subFrameset"/>
    </owned>
    <owned name="objInspFrame">
      <prop name="__CLASSNAME__" value="Oak::Web::Additional::ActionFrame"/>
      <prop name="target" value="objectInspector"/>
      <prop name="parent" value="subFrameset"/>
      <prop name="top" value="0"/>
      <prop name="left" value="0"/>
      <prop name="ev_onLoad" value="$::TL::actionPage->on_objInsp_click"/>
    </owned>
    <owned name="previewFrame">
      <prop name="__CLASSNAME__" value="Oak::Web::Additional::ActionFrame"/>
      <prop name="parent" value="subFrameset"/>
      <prop name="top" value="1"/>
      <prop name="left" value="0"/>
      <prop name="ev_onLoad" value="$::TL::actionPage->on_preview_click"/>
    </owned>
    <owned name="treeFrame">
      <prop name="__CLASSNAME__" value="Oak::Web::Additional::ActionFrame"/>
      <prop name="parent" value="subFrameset"/>
      <prop name="top" value="2"/>
      <prop name="left" value="0"/>
      <prop name="ev_onLoad" value="$::TL::actionPage->on_tree_click"/>
    </owned>
  </main>
